  "scripts": {
    "build": "npm run build:comment && npm run build:es3 && npm run build:es2015 && npm run build:ts",
    "build:comment": "node ./lib/generator/buildComment.js ./comment && npm run lint:comment:conf && npm run lint:comment:check",
    "build:es2015": "node ./lib/generator/build.js ./es2015 && npm run lint:es2015:conf && npm run lint:es2015:check",
    "build:es3": "node ./lib/generator/build.js ./es3 && npm run lint:es3:conf && npm run lint:es3:check",
    "build:ts": "node ./lib/generator/buildTypescript.js ./typescript && npm run lint:ts:conf && npm run lint:ts:check",
    "lint:comment:check": "eslint -c ./comment/configs/.eslintrc.json ./comment/configs/check.js --ignore-pattern '!'",
    "lint:comment:conf": "eslint -c ./comment/configs/.eslintrc.json ./comment/.eslintrc.js --fix --ignore-pattern '!'",
    "lint:es2015:check": "eslint -c ./es2015/configs/.eslintrc.json ./es2015/configs/check.js --ignore-pattern '!'",
    "lint:es2015:conf": "eslint -c ./es2015/configs/.eslintrc.json ./es2015/.eslintrc.js --fix --ignore-pattern '!'",
    "lint:es2015:rule": "eslint -c ./es2015/configs/.eslintrc.json ./es2015/rules",
    "lint:es3:check": "eslint -c ./es3/configs/.eslintrc.json ./es3/configs/check.js --ignore-pattern '!'",
    "lint:es3:conf": "eslint -c ./es3/configs/.eslintrc.json ./es3/.eslintrc.js --fix --ignore-pattern '!'",
    "lint:es3:rule": "eslint -c ./es3/configs/.eslintrc.json ./es3/rules",
    "lint:lib": "eslint -c ./lib/.eslintrc.json ./lib",
    "lint:ts:check": "eslint -c ./typescript/configs/.eslintrc.json ./typescript/configs/check.ts --ignore-pattern '!'",
    "lint:ts:conf": "eslint -c ./typescript/configs/.eslintrc.json ./typescript/.eslintrc.js --fix --ignore-pattern '!'",
    "watch:es2015": "node ./lib/generator/watch.js ./es2015",
    "watch:es3": "node ./lib/generator/watch.js ./es3"
  },
  "version": "2.1.0"
